    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^6.5",
        "illuminate/support": "^6.0",
        "illuminate/database": "^6.0",
        "laravolt/suitable": "^4.0",
        "laravolt/semantic-form": "^4.0",
        "bensampo/laravel-enum": "^1.26",
        "rkorebrits/htmltoopenxml": "^0.1.9",

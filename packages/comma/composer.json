{
    "name": "laravolt/comma",
    "description": "Content management for Laravel",
    "keywords": ["laravel", "laravolt", "blog", "CMS"],
    "homepage": "https://github.com/laravolt/comma",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com",
            "homepage": "http://id-laravel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/database": "^6.0",
        "illuminate/support": "^6.0",
        "spatie/laravel-sluggable": "^2.2",
        "spatie/laravel-medialibrary": "^7.0",
        "spatie/laravel-translatable": "^4.2",
        "spatie/laravel-tags": "^2.5",
        "spatie/laravel-schemaless-attributes": "^1.6",
        "spatie/laravel-activitylog": "^3.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0",
        "orchestra/database": "^4.0",
        "mockery/mockery": "^1.2"
    },
    "suggest": {
        "laravel/scout": "Required to utilize Searchable Post"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Comma\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Comma\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravolt\\Comma\\ServiceProvider"
            ]
        }
    }
}

{
    "name": "laravolt/support",
    "description": "Laravolt support",
    "keywords": ["laravel", "laravolt", "support"],
    "homepage": "https://github.com/laravolt/support",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "uyab.exe@gmail.com",
            "homepage": "https://laravolt.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^6.0",
        "illuminate/database": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Support\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Laravolt\\Support\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravolt\\Support\\SupportServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "4.x-dev"
        }
    }
}

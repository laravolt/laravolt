{
    "name": "laravolt/suitable",
    "description": "Semantic-UI table builder for Laravel application",
    "keywords": ["laravel", "laravolt", "table", "crud", "suitable", "semantic UI"],
    "homepage": "https://github.com/laravolt/suitable",
    "license": "MIT",
    "authors": [
        {
            "name": "Bayu Hendra Winata",
            "email": "bayu.hendra@javan.co.id",
            "homepage": "https://laravolt.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "illuminate/auth": "^6.0",
        "illuminate/support": "^6.0",
        "laravolt/support": "^4.0",
        "rap2hpoutre/fast-excel": "^1.3.1",
        "niklasravnsborg/laravel-pdf": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Laravolt\\Suitable\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev"
        },
        "laravel": {
            "providers": [
                "Laravolt\\Suitable\\ServiceProvider"
            ],
            "aliases": {
                "Suitable": "Laravolt\\Suitable\\Facade"
            }
        }
    }
}
